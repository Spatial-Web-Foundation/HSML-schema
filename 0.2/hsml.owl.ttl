@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .

<https://www.spatialwebfoundation.org/ns/hsml>
  rdf:type owl:Ontology ;
  owl:versionIRI <https://www.spatialwebfoundation.org/ns/hsml/0.2> ;
  dct:title "Hyperspace Modelling Language (HSML) Root Ontology" ;
  dct:description "This is the root ontology for HSML. It imports all constituent modules of the HSML standard."@en ;

  # Import all normative HSML modules
  owl:imports <https://www.spatialwebfoundation.org/ns/hsml/core> ;
  owl:imports <https://www.spatialwebfoundation.org/ns/hsml/agent> ;
  owl:imports <https://www.spatialwebfoundation.org/ns/hsml/activity> ;
  owl:imports <https://www.spatialwebfoundation.org/ns/hsml/governance> ;
  owl:imports <https://www.spatialwebfoundation.org/ns/hsml/hyperspace> .