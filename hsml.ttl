# baseURI: https://www.spatialwebfoundation.org/ns/hsml
#
# HSML Master Ontology
# --------------------
# This is an umbrella file that imports all the individual modules of the
# Hyperspace Modeling Language (HSML) specification. Load this file to
# get the complete HSML model.
#
# Version: 0.2
# Date: September 1, 2025

@prefix hsml: <https://www.spatialwebfoundation.org/ns/hsml#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .

<https://www.spatialwebfoundation.org/ns/hsml>
  rdf:type owl:Ontology ;
  owl:versionInfo "0.2" ;
  owl:versionIRI <https://www.spatialwebfoundation.org/ns/hsml/0.2/hsml> ;
  dct:created "2025-09-01"^^xsd:date ;
  dct:publisher <https://www.spatialwebfoundation.org/> ;
  rdfs:label "HSML Master Ontology" ;
  rdfs:comment "This ontology serves as the master file that imports all constituent modules of the Hyperspace Modeling Language." ;

  # Import all HSML modules
  owl:imports <https://www.spatialwebfoundation.org/ns/hsml/core> ;
  owl:imports <https://www.spatialwebfoundation.org/ns/hsml/activity> ;
  owl:imports <https://www.spatialwebfoundation.org/ns/hsml/agent> ;
  owl:imports <https://www.spatialwebfoundation.org/ns/hsml/governance> ;
  owl:imports <https://www.spatialwebfoundation.org/ns/hsml/comm> ;
  owl:imports <https://www.spatialwebfoundation.org/ns/hsml/hyperspace> ;

  # Import Hyperspace submodules explicitly for clarity
  owl:imports <https://www.spatialwebfoundation.org/ns/hsml/hyperspace/metric> ;
  #owl:imports <https://www.spatialwebfoundation.org/ns/hsml/hyperspace/topo> ;
  #owl:imports <https://www.spatialwebfoundation.org/ns/hsml/hyperspace/vector> ;
.